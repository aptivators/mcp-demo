# Sample .env file for configuration
# Copy this file to .env and fill in your actual credentials

# Microsoft Entra (Azure AD) OAuth2 credentials
ENTRA_CLIENT_ID=your-entre-client-id-here
ENTRA_CLIENT_SECRET=your-entra-client-secret-here
ENTRA_TENANT_ID=your-entra-tenant-id-here

# SharePoint configuration
SP_COMPANY=yourcompanyname
SP_SITEPATH=thesite-name
SP_FOLDER="Shared%20Documents/your-folder"

# Atlassian OAuth2 credentials
ATLASSIAN_CLIENT_ID=client-id-placeholder
ATLASSIAN_CLIENT_SECRET=client-secret-placeholder
ATLASSIAN_REDIRECT_URI=redirect-uri-placeholder

# Gemini AI configuration
GEMINI_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent
GEMINI_MODEL=gemini-1.5-flash-latest
GEMINI_API_KEY=your-gemini-api-key-here